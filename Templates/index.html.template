<!DOCTYPE html>
<html lang="en-us">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>Unity WebGL Player | {{utopen}} PRODUCT_NAME {{utclose}}</title>
  </head>
  <body style="text-align: center">
    <canvas id="unity-canvas" style="width: {{utopen}} WIDTH {{utclose}}px; height: {{utopen}} HEIGHT {{utclose}}px; background: {{utopen}} BACKGROUND_FILENAME ? 'url(\'Build/' + BACKGROUND_FILENAME.replace(/'/g, '%27') + '\') center / cover' : BACKGROUND_COLOR {{utclose}}"></canvas>
    <script src="Build/{{utopen}} LOADER_FILENAME {{utclose}}"></script>
    <script>
      const canvas = document.querySelector('#unity-canvas');
      canvas.style.width = '100%';
      canvas.style.height = '100%';

      window.addEventListener('load', resizePage, false);
      window.addEventListener('resize', resizePage, false);

      function resizePage() {
          canvas.style.width = window.innerWidth + 'px';
          canvas.style.height = window.innerHeight + 'px';
      }

      createUnityInstance(document.querySelector("#unity-canvas"), {
        dataUrl: "Build/{{utopen}} DATA_FILENAME {{utclose}}",
        frameworkUrl: "Build/{{utopen}} FRAMEWORK_FILENAME {{utclose}}",
        codeUrl: "Build/{{utopen}} CODE_FILENAME {{utclose}}",
#if MEMORY_FILENAME
        memoryUrl: "Build/{{utopen}} MEMORY_FILENAME {{utclose}}",
#endif
#if SYMBOLS_FILENAME
        symbolsUrl: "Build/{{utopen}} SYMBOLS_FILENAME {{utclose}}",
#endif
        streamingAssetsUrl: "StreamingAssets",
        companyName: "{{utopen}} COMPANY_NAME {{utclose}}",
        productName: "{{utopen}} PRODUCT_NAME {{utclose}}",
        productVersion: "{{utopen}} PRODUCT_VERSION {{utclose}}",
      });
    </script>
  </body>
</html>
